# comment in the same line of variable will cause the issue in extracing value

SERVER_PORT = 8000

POSTGRES_DB_USER = 'guru'
POSTGRES_DB_PASSWORD = 'guru'
POSTGRES_DB_HOST = 'localhost'
POSTGRES_DATABASE = 'jwt5'
POSTGRES_DB_PORT = '5432'

JWT_SECRET_KEY = default_dev_jwt_key_1234  
